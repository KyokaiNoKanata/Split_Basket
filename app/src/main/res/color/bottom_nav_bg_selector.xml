<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Pressed: deepen background color for tactile feedback -->
    <item android:color="@color/nav_active_bg_pressed" android:state_pressed="true" />
    <!-- Selected: active page background color -->
    <item android:color="@color/nav_active_bg" android:state_selected="true" />
    <!-- Default: transparent -->
    <item android:color="@android:color/transparent" />
</selector>
